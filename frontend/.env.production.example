# Configuración para frontend Next.js - hablaris.com

# API Configuration
NEXT_PUBLIC_API_URL=https://api.hablaris.com
NEXT_PUBLIC_BACKEND_URL=https://api.hablaris.com

# App Configuration  
NEXT_PUBLIC_APP_URL=https://hablaris.com
NEXT_PUBLIC_APP_NAME=Hablaris eSIM

# Environment
NEXT_PUBLIC_ENVIRONMENT=production

# Features
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PAYMENTS=true

# Domain specific
NEXT_PUBLIC_DOMAIN=hablaris.com
NEXT_PUBLIC_COMPANY_NAME=Hablaris

# Optional (configurar después si necesitas)
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_key
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
